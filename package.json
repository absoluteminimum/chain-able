{
  "name": "chain-able",
  "version": "4.0.0",
  "description": "interfaces that describe their intentions.",
  "main:es6": "src/index.js",
  "main:dev": "dists/dev/index.js",
  "main:tsc": "dists/tsc/index.js",
  "main:iife": "dists/iife/index.js",
  "main:umd": "dists/umd/index.js",
  "main:cjs": "dists/cjs/index.js",
  "main:es": "dists/es/index.js",
  "js:next": "dists/es/index.js",
  "main": "dists/umd/index.js",
  "module": "dists/umd/index.js",
  "web": "dists/cjs/index.js",
  "browser": "dists/cjs/index.js",
  "alias": "dists/cjs/index.js",
  "amd": "dists/amd/index.js",
  "types": "typings/index.d.ts",
  "typings": "typings/index.d.ts",
  "rollup": {
    "moduleName": "ChainAble",
    "entry": "src/index.js",
    "distEntry": "dist/index.js",
    "_tscEntry": "dists/tsc/index.js",
    "tscEntry": "dists/dev/index.js"
  },
  "scripts": {
    "cli": "node build/cli.js",
    "123": "npm run strip:all && buble:all && npm run cli",
    "rollcli": "rollup --config build/cli-rollup.js --environment format:dev",
    "optimize": "optimize-js dists/umd/index.js > dists/umd/index.js",
    "tsc": "tsc",
    "rollup": "rollup",
    "ava": "ava",
    "webpack": "webpack --verbose",
    "gzip": "gzip-size dists/umd/index.js --raw && gzip-size dists/umd/index.js --raw >> build/size-over-time.txt && date +%Y:%M:%D:%H:%M:%S >> build/size-over-time.txt && echo --- >> build/size-over-time.txt",
    "dox": "doxdox 'src/**/*.js' --layout markdown --output docs/bits/doxdox.md",
    "prepublish": "npm run build && npm run pretest && npm run cov && npm run test:built",
    "pretest": "npm run strip:all && npm run buble:all",
    "pre:test:only": "npm run buble:tests",
    "test:tests": "npm run pre:test:only && ava --verbose",
    "test": "ava --verbose",
    "tests": "ava --verbose",
    "test:built": "ava test-dist/built.js",
    "test:all": "npm run test && npm run test:built",
    "cov": "nyc ava",
    "cov:report": "nyc report",
    "cov:eralls": "coveralls < coverage/lcov.info",
    "cov:text": "nyc --reporter=html --reporter=text ava",
    "_build": "&& npm run optimize ",
    "build": "npm run strip:all && npm run cli && npm run gzip",
    "build:rollup": "rollup -c build/rollup.config.js",
    "buble": "buble -i dist -o dist --no forOf,dangerousForOf,computedProperty,spreadRest",
    "buble:sm": "buble -i dist -o dist -m inline --no forOf,dangerousForOf,computedProperty,spreadRest",
    "buble:tests": "buble -i test -o test-dist --no forOf,dangerousForOf,computedProperty,spreadRest",
    "buble:tests:sm": "buble -i test -o test-dist -m inline --no forOf,dangerousForOf,computedProperty,spreadRest",
    "buble:all": "npm run buble:sm && npm run buble:tests:sm",
    "babel:all": "npm run babel && npm run babel:tests",
    "babel": "babel src/ --out-dir dist",
    "babel:tests": "babel test/ --out-dir test-dist",
    "strip": "flow-remove-types src/ --pretty --all --out-dir ./ --quiet",
    "strip:dist": "flow-remove-types src/ --all --out-dir ./dist --quiet",
    "strip:all": "npm run strip && npm run strip:dist",
    "lint": "eslint src/**.js src/**/*.js"
  },
  "nyc": {
    "all": false,
    "check-coverage": true,
    "sourceMap": true,
    "instrument": false,
    "cache": false,
    "include": [
      "./dist/**/*.js"
    ],
    "exclude": [
      "_todos/**",
      "_play/**",
      ".github/**",
      "play/**",
      "dist/deps/*.js",
      "dist/deps/**/*.js",
      "**/deps/*.js",
      "**/deps/**",
      "test-dist/traverse/index.js",
      "test-dist/traverse/lib/deep_equal.js",
      "test-dist/built.js",
      "test-dist/chainsaw/*.js"
    ],
    "_require": [
      "babel-register",
      "babel-core/register"
    ],
    "lines": 85,
    "statements": 85,
    "functions": 85,
    "branches": 70,
    "extension": [
      ".js"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "ava": {
    "__files": [
      "test-dist/schema.js"
    ],
    "_files": [
      "test-dist/built.js",
      "test-dist/chainsaw/*.js"
    ],
    "files": [
      "!index.*.js",
      "test-dist/api.js",
      "test-dist/schema.js",
      "test-dist/Chainable.js",
      "test-dist/ChainedSet.js",
      "test-dist/compose.js",
      "test-dist/define.js",
      "test-dist/dot.js",
      "test-dist/examples-v1-pt1.js",
      "test-dist/examples-v1-pt2.js",
      "test-dist/extensions.js",
      "test-dist/factory.js",
      "test-dist/iteration.js",
      "test-dist/LocalStorage.js",
      "!test-dist/MergeChain.js",
      "test-dist/MobX.js",
      "test-dist/observe.js",
      "test-dist/parent.js",
      "test-dist/remap.js",
      "test-dist/shorthands.js",
      "test-dist/traverse.js",
      "test-dist/typed.js",
      "test-dist/webpack-chain-map.js",
      "test-dist/webpack-chain-set.js",
      "test-dist/dot.js",
      "test-dist/observe.js",
      "test-dist/deps/*.js",
      "test-dist/traverse/*.js",
      "test-dist/is/*.js",
      "!test-dist/is/index.js",
      "!test-dist/traverse/index.js",
      "!test-dist/traverse/lib/deep_equal.js",
      "!test-dist/built.js"
    ],
    "source": [
      "dist/**",
      "!src/**/*"
    ],
    "verbose": true,
    "concurrency": 3,
    "failFast": false,
    "failWithoutAssertions": false,
    "powerAssert": true,
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "ava2": {
    "files": [
      "test/*.js",
      "!index.*.js",
      "!test/dot.js",
      "!test/observe.js",
      "!test/deps/*.js",
      "!test/traverse/*.js",
      "!test/is/*.js",
      "!test/is/index.js",
      "!test/traverse/index.js",
      "!test/traverse/lib/deep_equal.js",
      "!test/built.js"
    ],
    "source": [
      "src/**/*",
      "src/*",
      "src/**",
      "dist/*",
      "dist/**",
      "dist/**/*"
    ],
    "verbose": true,
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "powerAssert": true,
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "_optionalDependencies": {
    "bench-chain": "^0.5.1"
  },
  "devDependencies": {
    "@types/node": "^7.0.31",
    "ava": "*",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "buble": "0.15.2",
    "chainsaw": "^0.1.0",
    "coveralls": "^2.13.1",
    "dargs": "^5.1.0",
    "deepmerge": "^1.3.2",
    "doxdox": "^2.0.3",
    "eslint": "^4.0.0",
    "eslint-config-chain-able": "^1.0.0",
    "eslint-plugin-typescript": "^0.1.0",
    "flipfile": "^0.0.13",
    "fliplog": "^1.0.0",
    "flow-remove-types": "^1.2.1",
    "funwithflags": "^1.0.2",
    "fuse-box": "2.2.0-beta.16",
    "gzip-size-cli": "^2.0.0",
    "immutable": "^3.8.1",
    "inspector-gadget": "^1.0.0",
    "mobx": "^3.1.14",
    "module-alias": "^2.0.0",
    "nyc": "^10.3.2",
    "optimize-js": "^1.0.3",
    "rollup": "^0.43.0",
    "rollup-plugin-babili": "^3.0.0",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-cleanup": "^1.0.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-filesize": "^1.3.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-typescript2": "^0.4.4",
    "rollup-plugin-uglify": "^2.0.1",
    "script-chain": "^0.0.9",
    "traverse": "^0.6.6",
    "tslint": "^5.4.3",
    "typescript": "next",
    "typescript-eslint-parser": "^3.0.0",
    "uglify-es": "^3.0.15",
    "uglify-js": "3.0.15",
    "validator": "^7.0.0",
    "webpack": "3.0.0"
  },
  "keywords": [
    "chains",
    "chaining",
    "chain-able",
    "conditional",
    "expressive",
