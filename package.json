{
  "name": "chain-able",
  "version": "4.0.0-beta.2",
  "description": "interfaces that describe their intentions.",
  "main:es6": "src/index.js",
  "main:dev": "dists/dev/index.js",
  "main:tsc": "dists/tsc/index.js",
  "main:iife": "dists/iife/index.js",
  "main:umd": "dists/umd/index.js",
  "main:cjs": "dists/cjs/index.js",
  "main:es": "dists/es/index.js",
  "js:next": "dists/es/index.js",
  "main": "dists/umd/index.js",
  "module": "dists/umd/index.js",
  "web": "dists/cjs/index.js",
  "browser": "dists/cjs/index.js",
  "alias": "dists/cjs/index.js",
  "amd": "dists/amd/index.js",
  "types": "typings/index.d.ts",
  "typings": "typings/index.d.ts",
  "rollup": {
    "moduleName": "ChainAble",
    "entry": "src/index.js",
    "distEntry": "dist/index.js",
    "_tscEntry": "dists/tsc/index.js",
    "tscEntry": "dists/dev/index.js",
    "rollupConfig": {
      "moduleName": "ChainAble",
      "entry": "src/index.js",
      "distEntry": "dist/index.js",
      "_tscEntry": "dists/tsc/index.js",
      "tscEntry": "dists/dev/index.js"
    }
  },
  "scripts": {
    "123": "npm run strip:all && buble:all && npm run cli",
    "___": " --runInBand",
    "jest": "jest",
    "cli": "node build/cli.js",
    "rollcli": "rollup --config build/cli-rollup.js --environment format:dev",
    "optimize": "optimize-js dists/umd/index.js > dists/umd/index.js",
    "tsc": "tsc",
    "rollup": "rollup",
    "ava": "ava",
    "webpack": "webpack --verbose",
    "gzip": "gzip-size dists/umd/index.js --raw && gzip-size dists/umd/index.js --raw >> build/size-over-time.txt && date +%Y:%M:%D:%H:%M:%S >> build/size-over-time.txt && echo --- >> build/size-over-time.txt",
    "dox": "doxdox 'src/**/*.js' --layout markdown --output docs/bits/doxdox.md",
    "prepublish": "npm run strip:root && npm run build && npm run pretest && jest --coverage && npm run test:built",
    "postpublish": "npm run cli -- --clean",
    "pretest": "npm run strip:dist && npm run buble",
    "pre:test:only": "npm run buble:tests",
    "test:tests": "npm run pre:test:only && ava --verbose",
    "test": "jest",
    "tests": "ava --verbose",
    "test:built": "ava test/built.js",
    "test:built:jest": "jest test/built.js --testPathIgnorePatterns=\"\" --testRegex=\"test/built.js\"",
    "cov": "nyc ava",
    "cov:report": "nyc report",
    "cov:eralls": "coveralls < coverage/lcov.info",
    "cov:text": "nyc --reporter=html --reporter=text ava",
    "_build": "&& npm run optimize ",
    "build": "npm run pretest && npm run cli && npm run gzip",
    "build:rollup": "rollup -c build/rollup.config.js",
    "buble": "buble -i dist -o dist --no forOf,dangerousForOf,computedProperty,spreadRest",
    "buble:sm": "buble -i dist -o dist -m inline --no forOf,dangerousForOf,computedProperty,spreadRest",
    "buble:tests": "buble -i test -o test-dist --no forOf,dangerousForOf,computedProperty,spreadRest",
    "buble:tests:sm": "buble -i test -o test-dist -m inline --no forOf,dangerousForOf,computedProperty,spreadRest",
    "buble:all": "npm run buble:sm && npm run buble:tests:sm",
    "babel:all": "npm run babel && npm run babel:tests",
    "babel": "babel src/ --out-dir dist",
    "babel:tests": "babel test/ --out-dir test-dist",
    "strip:root": "flow-remove-types src/ --pretty --all --out-dir ./ --quiet",
    "strip:dist": "flow-remove-types src/ --all --out-dir ./dist --quiet",
    "strip:all": "npm run strip && npm run strip:dist",
    "lint": "eslint src/**.js src/**/*.js"
  },
  "nyc": {
    "all": false,
    "check-coverage": true,
    "sourceMap": true,
    "instrument": false,
    "cache": false,
    "include": [
      "./dist/**/*.js"
    ],
    "exclude": [
      "_todos/**",
      "_play/**",
      ".github/**",
      "play/**",
      "dist/deps/*.js",
      "dist/deps/**/*.js",
      "**/deps/*.js",
      "**/deps/**",
      "test-dist/traverse/index.js",
      "test-dist/traverse/lib/deep_equal.js",
      "test-dist/built.js",
      "test-dist/chainsaw/*.js"
    ],
    "_require": [
      "babel-register",
      "babel-core/register"
    ],
    "lines": 85,
    "statements": 85,
    "functions": 85,
    "branches": 70,
    "extension": [
      ".js"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ]
  },
  "_____": [
    "/test/",
    "/src/"
  ],
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/",
      "/src/deps/_r|meta|external|env|primitives/",
      "/src/deps/external/lodash/",
      "/src/deps/external/",
      "/src/deps/cache/scoped.js",
      "/src/deps/class-names.js",
      "/src/deps/dopemerge/map.js",
      "/src/compose/decorators.js",
      "/src/conditional/",
      "/src/deps/util/flatten.js",
      "/src/deps/is/class|node|stringOrNumber.js",
      "/src/deps/reduce/objects.js",
      "/src/deps/symbols/index|species|spreadable.js",
      "/src/deps/prefix|index|uniq|eqCurry|typeof|insert-at-index|nonEnumerableTypes.js",
      "/src/plugins/index.js"
    ],
    "useStderr": true,
    "verbose": true,
    "globals": {
      "__DEV__": true
    },
    "watch": false,
    "cache": true,
    "setupTestFrameworkScriptFile": "./test/__testsetup.js",
    "collectCoverageFrom": [
      "src/**/*.js",
      "test/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "\\\\node_modules\\\\",
      "src/deps/_r|meta|external|env|primitives",
      "src/deps/external/lodash",
      "src/deps/external",
      "src/deps/cache/scoped.js",
      "src/deps/class-names.js",
      "src/deps/dopemerge/map.js",
      "src/deps/util/flatten|charCodeAtZero.js",
      "src/deps/is/class|node|stringOrNumber.js",
      "src/deps/reduce/objects.js",
      "src/deps/symbols/index|species|spreadable.js",
      "src/deps/prefix|index|uniq|eqCurry|typeof|insert-at-index|nonEnumerableTypes.js",
      "src/compose/decorators.js",
      "src/deps/conditional/bitwise",
      "src/deps/conditional/bitwise/bitwise.js",
      "src/deps/conditional/bitwise/ez.js",
      "src/plugins/index.js"
    ],
    "coverageReporters": [
      "json",
      "text",
      "lcov",
      "clover"
    ],
    "automock": false,
    "bail": false,
    "browser": false,
    "rootDir": ".",
    "testRegex": "(test/)(.*).js$",
    "testPathIgnorePatterns": [
      "node_modules",
      "test/__testsetup.js",
      "test/built.js",
      "test/_stress.js",
      "test/_api.js",
      "test/is/index.js",
      "test/MergeChain.js",
      "test/chainsaw",
      "test/traverse/index.js"
    ]
  },
  "ava": {
    "files": [
      "!index.*.js",
      "test-dist/*.js"
    ],
    "source": [
      "dist/**",
      "!src/**/*"
    ],
    "verbose": true,
    "concurrency": 3,
    "failFast": false,
    "failWithoutAssertions": false,
    "powerAssert": true,
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "_optionalDependencies": {
    "bench-chain": "^0.5.1"
  },
  "devDependencies": {
    "@types/node": "^7.0.31",
    "ava": "*",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.1",
    "babel-plugin-istanbul": "^4.1.4",
